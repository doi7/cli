{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Release",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}",
      "args": ["release", "-cp"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Release (With docker)",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["release", "-D", "container"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Favicons",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["favicons", "-s", "icon.png"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["launch", "-bpcRD"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch (allowUnknownOption)",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["launch", "--cron", "--from", "repository", "--bash"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch (Debug mode)",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["launch", "-p", "-D"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Liftoff",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["liftoff", "-e", "-c", "-p", "development"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Liftoff <command>",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["liftoff", "command", "-a", "-b"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Unknown Options",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/@do-it/cli/src/bin/doit.js",
      "cwd": "${workspaceFolder}/packages/test",
      "args": ["liftoff", "-x", "-p", "-c", "-e"]
    }
  ]
}